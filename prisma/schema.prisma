  datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
  }

  generator client {
    provider      = "prisma-client-js"
  }

  model User {
    id            Int       @default(autoincrement()) @id
    email         String    @unique
    password      String
    username      String
    profile_image String    @default("https://cdn-icons-png.flaticon.com/512/1077/1077114.png")
  }
